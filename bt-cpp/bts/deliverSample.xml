<?xml version="1.0" encoding="UTF-8"?>
<root BTCPP_format="4">
  <BehaviorTree ID="main">
    <Fallback>
      <Sequence>
        <Sequence name="GetSample">
          <GoToDestination x=""
                           y=""/>
          <Authenticate token=""/>
          <WaitForSample/>
        </Sequence>
        <GoToDestination x=""
                         y=""/>
        <Sequence name="DeliverSample">
          <Authenticate token=""/>
          <WaitForCollection/>
        </Sequence>
      </Sequence>
      <GoToDestination x=""
                       y=""/>
    </Fallback>
  </BehaviorTree>

  <!-- Description of Node Models (used by Groot) -->
  <TreeNodesModel>
    <Action ID="Authenticate"
            editable="true">
      <output_port name="token"/>
    </Action>
    <Action ID="GoToDestination"
            editable="true">
      <input_port name="x"/>
      <input_port name="y"/>
    </Action>
    <Action ID="WaitForCollection"
            editable="true"/>
    <Action ID="WaitForSample"
            editable="true"/>
  </TreeNodesModel>

</root>
